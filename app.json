{
    "expo": {
        "name": "roadhouse",
        "slug": "roadhouse",
        "version": "1.0.0",
        "orientation": "portrait",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.roadhouse.roadhouseApp"
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#ffffff"
            },
            "package": "com.roadhouse.roadhouseApp"
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        }
    }
}
